function solucao(palpite, palavra) {
	 let acertos = 0;

  for (let i = 0; i < palavra.length; i++) {
    if (palavra[i] === palpite) {
      acertos++;
    }
  }

  console.log (acertos);
}


function processData(input) {
    const [palpite, palavra] = input.split(" ");
  	solucao(palpite, palavra);
}
